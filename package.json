{
  "homepage": "http://andygreenhaw.github.io/my-app",
  "name": "react-weather-app",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.5.3",
    "chart.js": "^2.9.4",
    "express": "^4.16.3",
    "gh-pages": "^3.1.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.16",
    "prop-types": "^15.7.2"
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build && json -I -f package.json -e \"this.deployed = true \"",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndyGreenhaw/react-weather-app.git"
  },
  "bugs": {
    "url": "https://github.com/AndyGreenhaw/react-weather-app/issues"
  }
}
